<mvc:View controllerName="samplecap.project1.controller.Dashboard"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:card="sap.f.cards"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:field="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <Page title="{i18n>dashboard}">
      <GenericTile class="sapUiLargeMarginBegin sapUiLargeMarginTop tileLayout" header="Application"
		subheader="Sample App" press="press" url="https://www.sap.com/">
		<TileContent>
			<ImageContent src="test-resources/sap/m/demokit/sample/GenericTileAsLaunchTile/images/SAPLogoLargeTile_28px_height.png" />
		</TileContent>
	</GenericTile>
    </Page>
</mvc:View>
